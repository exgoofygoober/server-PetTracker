{
    "version": 2,
    "builds": [
      {
        "src": "storage.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/encrypted/(.*)", "dest": "storage.js" },
      { "src": "/api/data/(.*)", "dest": "storage.js" },
      { "src": "/(.*)", "dest": "storage.js" }
    ]
  }